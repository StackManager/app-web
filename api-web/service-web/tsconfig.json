{
"compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./build",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "paths": {
      "@App":["app.ts"],
      "@Commons/*": ["commons/*"],
      "@DB/*": ["database/*"],
      "@Components/*": ["modules/web.components/*"],
      "@WorkSpaceComponents/*": ["modules/work.space.components/*"],
      "@WorkSpacePages/*": ["modules/work.space.pages/*"],
    }
  }
}